<h1>List of products</h1>

<ul>
<% @products.each do |p| %>

  <div class="name">
    <li> <%= p.name %></li>
  </div>


  <div class="name">
    <li> <%= p.description %></li>
  </div>


  <div class="name">
    <li> <%= p.price_in_cents %></li>
  </div>


<%end%>
</ul>

<%= link_to 'Create Product', new_product_path %>
